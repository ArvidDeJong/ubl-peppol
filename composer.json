{
    "name": "darvis/ubl-peppol",
    "description": "UBL/PEPPOL service voor het genereren van facturen volgens de UBL/PEPPOL standaard",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "ubl",
        "peppol",
        "e-facturatie",
        "factuur",
        "invoice",
        "xml",
        "laravel"
    ],
    "homepage": "https://github.com/ArvidDeJong/ubl-peppol",
    "authors": [
        {
            "name": "Arvid De Jong",
            "email": "info@arvid.nl",
            "homepage": "https://arvid.nl",
            "role": "Developer"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.2",
        "ext-dom": "*"
    },
    "require-dev": {
        "pestphp/pest": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Darvis\\UblPeppol\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Darvis\\UblPeppol\\UblPeppolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "support": {
        "issues": "https://github.com/ArvidDeJong/ubl-peppol/issues",
        "source": "https://github.com/ArvidDeJong/ubl-peppol"
    }
}